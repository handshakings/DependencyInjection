{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "readme": {
    "apiKey": "Your Readme APIKEY",
    "group": {
      "apiKey": "User Unique ID",
      "label": "User Name",
      "email": "User Email"
    },
    "options": {
      "allowList": "",
      "denyList": [ "password", "secret" ],
      "development": true,
      "bufferLength": 1,
      "baseLogUrl": "https://example.readme.com"
    }
  }
  

}
